{
  "name": "hubot",
  "version": "0.0.0-development",
  "author": "hubot",
  "keywords": [
    "github",
    "hubot",
    "campfire",
    "bot"
  ],
  "description": "A simple helpful robot for your Company",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hubotio/hubot.git"
  },
  "dependencies": {
    "connect-multiparty": "^2.2.0",
    "express": "^4.19.2",
    "express-basic-auth": "^1.2.1",
    "pino": "^8.11.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.2",
    "rimraf": "^6.0.1",
    "standard": "^17.1.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">= 18",
    "npm": ">= 9"
  },
  "main": "./index.mjs",
  "bin": {
    "hubot": "./bin/hubot"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "bin/hubot",
    "gen": "bin/hubot --create myhubot",
    "pretest": "standard",
    "test": "node --test",
    "test:smoke": "node src/**/*.js",
    "test:e2e": "bin/e2e-test.sh",
    "watch": "tsc --watch"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ],
    "dryRun": false
  }
}
