{
  "name": "hubot",
  "version": "1.0.0",
  "author": "hubot",
  "keywords": [
    "github",
    "hubot",
    "campfire",
    "bot"
  ],
  "description": "A simple helpful robot for your Company",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hubotio/hubot.git"
  },
  "type": "module",
  "dependencies": {
    "cline": "^0.8.2",
    "connect-multiparty": "^2.1.1",
    "express": "^4.16.3",
    "optparse": "1.0.4"
  },
  "devDependencies": {
    "chai": "~2.1.0",
    "coveralls": "^3.0.2",
    "is-circular": "^1.0.2",
    "mocha": "^9.2.2",
    "mockery": "^1.4.0",
    "nyc": "^15.1.0",
    "semantic-release": "^19.0.2",
    "sinon": "~1.17.0",
    "sinon-chai": "^2.8.0"
  },
  "engines": {
    "node": "> 17.0.0",
    "npm": "> 8.0.0"
  },
  "main": "./index.mjs",
  "bin": {
    "hubot": "./bin/hubot.mjs"
  },
  "scripts": {
    "ci": "node version",
    "start": "node bin/hubot.mjs",
    "local": "node bin/hubot.mjs",
    "test": "mocha --parallel",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:smoke": "node src/**/*.mjs",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  }
}
