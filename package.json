{
  "name": "hubot",
  "version": "6.0.1",
  "author": "hubot",
  "keywords": [
    "github",
    "hubot",
    "chatops",
    "bot"
  ],
  "description": "A simple helpful robot for your Company",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joeyguerra/hubot.git"
  },
  "type": "module",
  "dependencies": {
    "cline": "^0.8.2",
    "connect-multiparty": "^2.1.1",
    "express": "^4.16.3",
    "optparse": "1.0.4"
  },
  "engines": {
    "node": ">=v19.7.0",
    "npm": ">=v9.5.0"
  },
  "main": "./index.mjs",
  "bin": {
    "hubot": "./bin/hubot.mjs"
  },
  "scripts": {
    "start": "bun --hot bin/hubot.mjs",
    "test": "bun test",
    "start:local": "PORT=3000 bun --hot bin/hubot.mjs --adapter ../src/adapters/shell.mjs",
    "test:smoke": "bun src/**/*.mjs"
  }
}
